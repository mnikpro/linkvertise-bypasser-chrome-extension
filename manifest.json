{
    "manifest_version": 2,
    "name": "LinkVertise Byppasser",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["main.js"]
        }
    ],
    "description": "Bypasses linkvertise",
    "icons": {
        "16": "imgs/logo.png",
        "48": "imgs/logo.png",
        "128": "imgs/logo.png"
    },
    "browser_action": {
        "default_popup": "popup.html"
    }
}